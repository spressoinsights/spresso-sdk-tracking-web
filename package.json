{
    "name": "spresso-sdk-tracking-web",
    "version": "4.3.30-beta",
    "description": "Spresso Analytics Web SDK",
    "main": "index.js",
    "scripts": {
        "reset": "rimraf dist node_modules/.cache",
        "start": "webpack serve",
        "prestart-prod-mode": "npm run reset",
        "start-prod-mode": "NODE_ENV=production USE_NODEMON=true webpack --watch",
        "prebuild": "npm run reset",
        "build": "webpack",
        "dev:mb": "mb",
        "dev:setup": "babel-node ./test/setup/index.js",
        "dev:prod-mode": "run-p dev:mb dev:setup start-prod-mode",
        "dev": "run-p dev:mb dev:setup start",
        "docs:clear": "rimraf docs",
        "docs:generate-readme": "node scripts/generate-readme/index.js",
        "docs:build": "jsdoc -c jsdoc.config.js",
        "docs:clear-and-build": "run-s docs:clear docs:generate-readme docs:build",
        "docs": "nodemon --exec \"run-s docs:generate-readme docs:build\" --watch src --watch jsdoc.config.js --verbose --ext js,ts,md",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Suhan Wijaya",
    "license": "ISC",
    "devDependencies": {
        "@babel/node": "^7.16.8",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.18.6",
        "@types/uuid": "^8.3.4",
        "babel-loader": "^8.2.3",
        "better-docs": "^2.7.2",
        "core-js": "^3.21.1",
        "cross-fetch": "^3.1.5",
        "ejs": "^3.1.8",
        "get-port": "^5.1.1",
        "jsdoc": "^3.6.10",
        "mountebank": "^2.6.0",
        "nodemon": "^2.0.19",
        "nodemon-webpack-plugin": "^4.8.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "serve": "^14.0.1",
        "superagent": "^7.1.1",
        "terser-webpack-plugin": "^5.3.1",
        "ts-loader": "^9.3.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.7.4",
        "uglify-js": "^3.15.3",
        "uuid": "^8.3.2",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.3"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 150,
        "arrowParens": "always"
    }
}
