{
    "name": "spresso-sdk-tracking-web",
    "version": "1.1.18-alpha",
    "description": "Spresso Analytics Web SDK",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve",
        "start-prod-mode": "NODE_ENV=production nodemon --exec npm run build-and-serve --watch src",
        "prebuild": "rimraf dist",
        "build": "webpack --progress=profile",
        "serve": "serve dist --listen=3002",
        "build-and-serve": "run-s build serve",
        "dev:mb": "mb",
        "dev:setup": "babel-node ./test/setup/index.js",
        "dev:prod-mode": "run-p dev:mb dev:setup start-prod-mode",
        "dev": "run-p dev:mb dev:setup start",
        "docs:clear": "rimraf docs",
        "docs:generate-readme": "node src/scripts/generate-readme/index.js",
        "docs:build": "jsdoc -c jsdoc.config.js src/index.js",
        "docs:clear-and-build": "run-s docs:clear docs:generate-readme docs:build",
        "docs": "nodemon --exec npm run docs:clear-and-build --watch src",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Suhan Wijaya",
    "license": "ISC",
    "devDependencies": {
        "@babel/node": "^7.16.8",
        "@babel/preset-env": "^7.16.11",
        "babel-loader": "^8.2.3",
        "clean-jsdoc-theme": "^3.3.3",
        "core-js": "^3.21.1",
        "cross-fetch": "^3.1.5",
        "ejs": "^3.1.8",
        "get-port": "^5.1.1",
        "jsdoc": "^3.6.10",
        "mountebank": "^2.6.0",
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "serve": "^14.0.1",
        "superagent": "^7.1.1",
        "terser-webpack-plugin": "^5.3.1",
        "uglify-js": "^3.15.3",
        "uuid": "^8.3.2",
        "webpack": "^5.69.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 150,
        "arrowParens": "always"
    }
}
